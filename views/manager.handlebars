    <div style="padding-left:16px">
      <h2>Tropical Fish Emporium</h2>
      <p>Here customers, employees, sales, and products can be added to the database.</p>
      <form method = "post">
<!--CUSTOMERS-->
        <fieldset>
          <legend>Add Customer Here:</legend>
            <label>First Name:
              <input type ="text" name ="custFirstName">
            </label><br><br>
            <label>Last Name:
              <input type ="text" name ="custLastName">
            </label><br><br>
            <label>E-mail Address:
              <input type ="text" name ="email">
            </label><br><br>
            <label>Member Since:
              <input type ="date" name ="memberSince">
            </label><br><br>
            <input name ="custClick" id="custClick" value="Submit" type="Submit"/>
              <div id="custTy"></div>
        </fieldset>

  <!--EMPLOYEES-->
        <fieldset>
          <legend>Add Employee Here:</legend>
            <label>First Name:
              <input type ="text" name ="empFirstName">
            </label><br><br>
            <label>Last Name:
              <input type ="text" name ="empLastName">
            </label><br><br>
            <label>StoreID:
              <input type ="number" name ="storeID">
            </label><br><br>
            <label>Title:
              <input type ="text" name ="title">
            </label><br><br>
            <label>Shift Start-Time:
              <input type ="time" name ="startTime">
            </label><br><br>
            <label>Shift End-Time:
              <input type ="time" name ="stopTime">
            </label><br><br>
            <label>Hourly Rate:
              <input type ="number" name ="hourlyRate">
            </label><br><br>
            <label>Part-Time?</label>
              <select name = "partTime">
                      <option value = "1" selected> Yes</option>
                      <option value = "0" selected> No</option>
              </select><br><br>
              <input name ="empClick" id="empClick" value="Submit" type="Submit"/>
              <div id="empTy"></div>
        </fieldset>

  <!--PRODUCTS-->
        <fieldset>
          <legend>Add Product Here:</legend>
            <label>Product Name:
              <input type ="text" name ="pName">
            </label><br><br>
            <label>Price
              <input type ="price" name ="price">
            </label><br><br>
            <input name ="prodClick" id="prodClick" value="Submit" type="Submit"/>
              <div id="prodTy"></div>
        </fieldset>

  <!--SALES-->
        <fieldset>
          <legend>Add Sale Here:</legend>
            <label>Employee ID (who comlpeted the sale)
              <select  name="empID" id="eEmpID">
                {{#if emps}}
                  {{#each emps}}
                    <option value={{id}}>{{id}}: {{fName}} {{lName}}</option>
                  {{/each}}
                {{/if}}
              </select>
            </label>
            <strong>this is a dynamically populated list of employees in the database.</strong><br><br>
            <label>Customer ID:
              <select  name="custID" id="sCustomerID">
                {{#if custs}}
                  {{#each custs}}
                    <option value={{id}}>{{id}}: {{fName}} {{lName}}</option>
                  {{/each}}
                {{/if}}
              </select>
            </label>
            <strong>this is a dynamically populated list of customers in the database.</strong><br><br>
            <label>Transaction Date:
              <input type ="date" name ="transactionDate">
            </label><br><br>
            <label>Purchase Total (including tax):
              <input type ="price" name ="totalPurchase">
            </label><br><br>
            <input name ="salClick" id="salClick" value="Submit" type="submit"></input> 
        </fieldset>

<!--SALES_PRODUCTS-->
        <fieldset>
          <legend>Add sales_product Here:</legend>
            <label>Sale ID
              <select  name="salID" id="spSalID">
                {{#if spSal}}
                  {{#each spSal}}
                    <option value={{id}}>{{id}}: {{tDate}}</option>
                  {{/each}}
                {{/if}}
              </select>
            </label>
            <strong>this is a dynamically populated list of sales from the database.</strong><br><br>
            <label>Product ID:
              <select  name="prodID" id="spProdID">
                {{#if spProd}}
                  {{#each spProd}}
                    <option value={{id}}>{{id}}: {{pName}}</option>
                  {{/each}}
                {{/if}}
              </select>
            </label>
            <strong>this is a dynamically populated list of products from the database.</strong><br><br>
            <label>Quantity
              <input type ="quantity" name ="quantity">
            </label><br><br>
            <input name ="spClick" id="spClick" value="Submit" type="Submit"/>
              <div id="spTy"></div>
        </fieldset>
    </form>
    </div>
